	<queryString>
		<![CDATA[select
	d.DocumentDetailsID,
	d.DocumentID,
	d.ProductID,
	p.Code,
	p.Name,
	product_stock(p.ProductID,(select Date 
							 from document 
							where DocumentID = $P{documentId} ),1) +
	(select dd.Quantity 
	   from documentdetails dd
	 where dd.DocumentID = $P{documentId}
	       and dd.ProductID = d.ProductID) as pocetno,
	d.Quantity as razlika
  from documentdetails d
  left join product p on p.ProductID = d.ProductID
 where DocumentID =  $P{documentId}
 order by p.Code]]>
	</queryString>
